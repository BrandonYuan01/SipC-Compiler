main() {
    var a, b, c, d, e, f, g;

    //base
    a = 0;
    for (b : 1 .. 5 by 1) {
        a++;
    }
    if (a != 5) error 1;

    //nested
    c = 0;
    for (d : 1 .. 5 by 1) {
        for (e : 1 .. 5 by 1) {
            c++;
        }
    }
    if (c != 25) error 2;

    //array indexing
    f = [0, 1, 2, 3, 4, 5, 6];
    for (g : 0 .. 7 by 1) {
        f[g] = 100;
    }
    if (f[6] != 100) error 3;

    return 0;
}